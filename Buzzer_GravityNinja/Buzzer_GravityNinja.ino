const int boton = 2;
const int tonePin = 5;
int flag = 0;
void(* resetSoftware)(void) = 0;

void setup() {
  Serial.begin(9600);
  pinMode(boton, INPUT);
  pinMode(tonePin, OUTPUT);
  noTone(tonePin);
  attachInterrupt(digitalPinToInterrupt(boton),Reset,FALLING);
}

void loop() {
  if(Serial.available()){
    flag = Serial.read();
  }
  Serial.println(flag);
  if(flag == 49) {
    MenuMusic();
    
  }

  if(flag == 50) {
    RaceMusic();
  }

  if(flag == 51) {
    LoseMusic();
  }
  
  noTone(tonePin);
}

